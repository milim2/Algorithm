
function bubbleSort(arr) {
    let result = [...arr]; // copy the original data

    for (let i = 0; i < result.length - 1; i++) {
        for (let j = 0; j < result.length - 1; j++) {
            if (result[j] > result[j+1] {
                let temp = result[j];
                result[j] = result[j+1];
                result[j+1] = temp
            }
        }
    }

    return result;
}

let items = [5,9,12,2,4,8,10,7,3,1];
bubbleSort(items);
